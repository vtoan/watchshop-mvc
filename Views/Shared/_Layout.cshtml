<!DOCTYPE html>
<html lang="vi">
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="language" content="VN">
    <meta name="resource-type" content="document">
    <!--SEO-->
    <title>Hommer House - @ViewData["Title"]</title>
    <meta name="description" content="@ViewBag.Descripton">
    <meta name="copyright" content="Cửa hàng đồ hồ Hommer House"/>
    <meta name="author" content="Cửa hàng đồ hồ Hommer House"/>
    <!--OPEN GRAPH-->
    <meta property="og:title" content="@ViewData["Title"]" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://htmlcheatsheet.com/" />
    <meta property="og:image" content="https://htmlcheatsheet.com/images/html-cheatsheet.jpg" />
    <meta property="og:description" content="@ViewBag.Descripton" />
    <meta property="og:locale" content="vi_VN" />
    <!--CDN REFERENCE-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;1,100;1,300;1,400&display=swap" > 
    <link rel="stylesheet" href="https://maxst.icons8.com/vue-static/landings/line-awesome/line-awesome/1.3.0/css/line-awesome.min.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap-grid.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/normalize.css@8.0.1/normalize.css" >
    <!--CSS-->
    <link rel="stylesheet" href="~/css/main.css" />
    @RenderSection("css", false)
</head>
<body>
    <partial name="_PartialHeader"></partial>
    <main>@RenderBody()</main>
    <partial name="_PartialFooter"></partial>
    <!--SCRIPTS-->
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"
			integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
            crossorigin="anonymous"></script>
    <div id="fb-root"></div>
    <script async defer crossorigin="anonymous" src="https://connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v8.0" nonce="OKUxEWXb"></script>
    <script src="~/js/main.js"></script>
    <script src="~/js/cookie.js"></script>
    @RenderSection("scripts", false)
</body>
</html>
